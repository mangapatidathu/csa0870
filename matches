def matches(str1, str2):
    if len(str1) != len(str2):
        return "Error: Strings must have the same length"
    match_count = 0
    for i in range(len(str1)):
        if str1[i] == str2[i]:
            match_count += 1
    return match_count
string1 = "hello"
string2 = "hallo"
print("Number of matches:", matches(string1, string2))
